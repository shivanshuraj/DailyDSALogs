//bf failed
class Solution{
    //Function to count subarrays with sum equal to 0.
    public static long findSubarray(long[] arr ,int n) 
    {
        //Your code here
        long count=0;
        for(int i=0; i<n; i++){
            long sum=0;
            for(int j=i; j<n; j++){
                sum+=arr[j];
                if(sum==0){
                    count++;
                }
            }
        }
        return count;
    }
}
